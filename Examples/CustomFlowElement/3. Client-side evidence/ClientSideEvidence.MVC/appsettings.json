{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PipelineOptions": {
    "Elements": [
      {
        // The first Flow Element must handle the data manipulation prior to 
        // the others preparing JavaScript and Json responses.
        "BuilderName": "SimpleClientSideElement"
      },
      {
        // The JsonBuilderElement produces a JSON version of the flowdata. 
        // It should be placed after all data elements.
        "BuilderName": "JsonBuilderElement"
      },
      {
        // The JavaScriptBuilderElement creates some JavaScript code that
        // is used to expose the json data from the JsonBuilderElement in
        // client-side code and retrieve additional evidence values 
        // asynchronously on the client.
        "BuilderName": "JavaScriptBuilderElement"
      }
    ],
    // True if client-side properties should be enabled. If enabled
    // (and the JavaScriptBundlerElement added to the Pipeline), a
    // client-side JavaScript file will be served at the URL
    // */51Degrees.core.js.
    "ClientSideEvidenceEnabled": true,
    // Flag to enable/disable the use of the async attribute for
    // the client side script.
    "UseAsyncScript": true,
    // Flag to enable/disable a feature that will automatically set
    // the values of HTTP headers in the response in order to request
    // additional information.
    "UseSetHeaderProperties": true
  }
}